no shutdown

interface Tunnel 1
ipv6 address 2002:A:A:22::1/64 
tunnel source 200.1.1.195
tunnel destination 200.1.0.193 
tunnel mode ipv6ip
exit

ipv6 route 2002:A:A:6::/64 Tunnel 1

end
conf t

ipv6 router ospf 1
