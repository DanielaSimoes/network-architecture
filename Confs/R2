int f1/0
ip nat inside
int f0/1
ip nat inside
int f0/0
ip nat inside

int f1/1
ip nat outside

ip nat translation timeout 120
ip nat inside source list 2 pool MYNATPOOL overload

interface FastEthernet 0/0
ip address 10.1.200.3 255.255.255.252
ipv6 address 2002:A:A:18::3/64
ipv6 ospf 1 area 0
no shutdown

interface FastEthernet 0/1
