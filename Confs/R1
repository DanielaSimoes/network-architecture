ip nat inside source list 2 pool MYNATPOOL

int f1/0
ip nat inside
int f0/1
ip nat inside
int f0/0
ip nat inside

int f1/1
ip nat outside

ip nat translation timeout 120
ip nat inside source list 2 pool MYNATPOOL overload

interface FastEthernet 0/0
ip address 10.1.184.2 255.255.255.252
no shutdown

interface FastEthernet 0/1
ip address 10.1.200.2 255.255.255.252
no shutdown

interface FastEthernet 1/0
no switchport
ip address 10.1.208.1 255.255.255.252
no shutdown

interface FastEthernet 1/1
no switchport
ip address 200.1.1.193 255.255.255.224
no shutdown

end
conf t

